{
  "name": "contributte/doctrine-project",
  "description": "Minimal viable project with Doctrine to Nette (@nette) by @f3l1x",
  "keywords": [
    "nette",
    "skeleton",
    "project",
    "doctrine",
    "database",
    "dbal",
    "orm"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.2",

    "nette/application": "^3.2.3",
    "nette/bootstrap": "^3.2.3",
    "nette/caching": "^3.2.3",
    "nette/di": "^3.2.2",
    "nette/forms": "^3.2.3",
    "nette/http": "^3.3.0",
    "nette/utils": "^4.0.4",
    "latte/latte": "^3.0.16",
    "tracy/tracy": "^2.10.7",

    "contributte/console": "^0.10.1",
    "contributte/event-dispatcher": "^0.9.1",

    "nettrine/dbal": "^0.8.2",
    "nettrine/orm": "^0.8.4",
    "nettrine/cache": "^0.3.0"
  },
  "require-dev": {
    "contributte/tester": "^0.3.0",
    "symfony/thanks": "^1.4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "symfony/thanks": true
    }
  }
}
